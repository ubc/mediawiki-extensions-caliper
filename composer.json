{
    "name": "ubc/mediawiki-extensions-caliper",
    "type": "mediawiki-extension",
    "description": "The Caliper extension emits caliper events to a LRS.",
    "keywords": [
        "Caliper",
        "Wiki",
        "MediaWiki"
    ],
    "homepage": "https://github.com/ubc/mediawiki-extensions-caliper",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Andrew Gardener",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
              "name": "imsglobal/caliper",
              "version": "1.2.0",
              "dist": {
                "type": "zip",
                "url" : "https://github.com/ubc/mediawiki-extensions-caliper/raw/master/packages/caliper-php.zip"
              },
              "autoload": {
                "psr-4": {
                  "IMSGlobal\\Caliper\\": "src/"
                }
              },
              "_comment": {
                "source": {
                  "url": "https://github.com/IMSGlobal/caliper-php",
                  "type": "git",
                  "reference": "dev-develop#c7e34e230abc7bbb647f8f94245cb649f6191bcd"
                }
              }
            }
        }
    ],
    "require": {
        "imsglobal/caliper": "1.2.0"
    }
}
